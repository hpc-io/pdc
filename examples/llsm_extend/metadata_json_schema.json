{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "dataset_name": {
            "type": "string"
        },
        "dataset_description": {
            "type": "string"
        },
        "source_URL": {
            "type": "string"
        },
        "collector": {
            "type": "string"
        },
        "objects": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "type": {
                        "type": "string"
                    },
                    "full_path": {
                        "type": "string"
                    },
                    "properties": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "name": {
                                    "type": "string"
                                },
                                "value": {
                                    "oneOf": [
                                        {
                                            "type": "string"
                                        },
                                        {
                                            "type": "integer"
                                        },
                                        {
                                            "type": "number"
                                        }
                                    ]
                                },
                                "class": {
                                    "type": "string"
                                },
                                "type": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "name",
                                "value",
                                "class",
                                "type"
                            ]
                        }
                    }
                },
                "required": [
                    "name",
                    "properties"
                ]
            }
        }
    },
    "required": [
        "dataset_name",
        "objects"
    ]
}